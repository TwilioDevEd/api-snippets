curl -X POST 'https://api.twilio.com/2010-04-01/Accounts/{{#accountSid}}{{accountSid}}{{/accountSid}}/Messages.json' \
--data-urlencode 'To={{#toPhoneNumber}}{{toPhoneNumber}}{{/toPhoneNumber}}'  \
--data-urlencode 'From={{#fromPhoneNumber}}{{fromPhoneNumber}}{{/fromPhoneNumber}}'  \
--data-urlencode 'Body={{#messageBody}}{{messageBody}}{{/messageBody}}'  \
-d 'MediaUrl={{#messageMediaUrl}}{{messageMediaUrl}}{{/messageMediaUrl}}' \
-u {{#accountSid}}{{accountSid}}{{/accountSid}}:{{#authToken}}{{authToken}}{{/authToken}}
